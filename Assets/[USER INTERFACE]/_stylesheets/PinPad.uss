VisualElement.NumButton {
    align-self: center;
    justify-content: flex-start;
    flex-shrink: 0;
    padding-left: 0;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    width: 75px;
    height: 75px;
    max-width: 75px;
    max-height: 75px;
    transition-duration: 0.1s;
}

GroupBox.Row {
    flex-grow: 1;
    flex-direction: row;
    justify-content: space-evenly;
}

.Hovered {
    background-color: rgba(197, 163, 0, 0.73);
}

.Selected {
    background-color: rgba(255, 150, 0, 0.81);
}

.UnHovered {
    background-color: rgba(102, 147, 84, 0.82);
}
