SelectableButton {
    background-image: url("project://database/Assets/[USER%20INTERFACE]/PauseMenu/bg_folders/ui-folder-1-body2.png?fileID=1445594954290753641&guid=e9f3108207bed5c4caa303e96ced19e3&type=3#ui-folder-1_0");
