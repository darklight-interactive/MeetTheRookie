    flex-grow: 1;
    width: 3%;
    -unity-background-scale-mode: scale-to-fit;
    flex-shrink: 1;
    -unity-background-image-tint-color: rgb(255, 255, 255);
    background-color: rgba(0, 0, 0, 0);
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
    transition-duration: 0.3s;
    rotate: 0;
    transition-property: scale;
    scale: 1.1 1;
}

SelectableSlider .unity-base-slider__dragger.selected {
    background-image: url("project://database/Assets/[USER%20INTERFACE]/PauseMenu/Ui_Pin.png?fileID=4690419380084828854&guid=d5d4be384c1732247bb9fe36b6bcbacf&type=3#Ui_Pin_0");
    height: 25%;
    flex-grow: 1;
    width: 3%;
    -unity-background-scale-mode: scale-to-fit;
    flex-shrink: 1;
    -unity-background-image-tint-color: rgb(255, 255, 255);
    background-color: rgba(0, 0, 0, 0);
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
    scale: 1.3 1.3;
    transition-duration: 0.3s, 0.3s;
    translate: 0 0;
    rotate: 360deg;
    transition-property: scale, rotate;
}

