.clicked {
    border-left-color: rgb(0, 0, 0);
    border-right-color: rgb(0, 0, 0);
    border-top-color: rgb(0, 0, 0);
    border-bottom-color: rgb(0, 0, 0);
    border-top-width: 10px;
    border-right-width: 10px;
    border-bottom-width: 10px;
    border-left-width: 10px;
}

.icon {
    max-width: 128px;
    max-height: 128px;
    height: 128px;
    width: 128px;
    position: absolute;
}

#background {
}

#header {
    flex-basis: 25%;
}

#body {
    flex-basis: 100%;
    flex-direction: row;
    flex-grow: 1;
    align-items: flex-start;
    align-self: flex-start;
}

.selected {
    transition-duration: 0.25s;
    transition-timing-function: ease-in;
    scale: 1.1 1.1;
    rotate: 1deg;
    -unity-background-image-tint-color: rgba(255, 188, 0, 0.6);
}

#footer {
    flex-basis: 25%;
    align-items: center;
}

#background {
    flex-grow: 1;
    justify-content: center;
    align-self: center;
    width: 100%;
    height: 100%;
    background-color: rgb(30, 29, 57);
    position: absolute;
}

GroupBox {
    flex-grow: 1;
    justify-content: space-around;
    padding-top: 10px;
    padding-right: 10px;
    padding-bottom: 10px;
    padding-left: 10px;
}

VisualElement {
    -unity-background-scale-mode: scale-to-fit;
}

VisualElement.panel {
    flex-grow: 1;
    align-self: stretch;
    background-color: rgba(255, 255, 255, 0);
    -unity-background-scale-mode: stretch-to-fill;
    transition-duration: 0.3s;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    opacity: 0;
    display: none;
    translate: 0 0;
}

SelectableButton {
    background-image: url("project://database/Assets/[SPRITES]/_ui/ui_boxes.png?fileID=7526846761729532893&guid=87d54583fce2d4948a580954f560d140&type=3#ui_boxes_0");
    align-self: stretch;
    flex-grow: 0;
    font-size: 65px;
    margin-top: 10px;
    margin-right: 10px;
    margin-bottom: 10px;
    margin-left: 10px;
}

.unity-text-element {
    -unity-font-definition: resource('Fonts/digital_disco/DigitalDisco-Thin');
    white-space: normal;
}

Label.unity-text-element.title {
    font-size: 128px;
    color: rgb(255, 255, 255);
    -unity-font-definition: resource('Fonts/digital_disco/DigitalDisco');
    white-space: nowrap;
}

Label.unity-text-element.h1 {
    font-size: 96px;
    color: rgb(255, 255, 255);
    -unity-font-definition: resource('Fonts/digital_disco/DigitalDisco');
    white-space: nowrap;
}

Label.unity-text-element.h2 {
    font-size: 69px;
    color: rgb(255, 255, 255);
    -unity-font-definition: resource('Fonts/digital_disco/DigitalDisco');
    white-space: nowrap;
}

Label.unity-text-element.h3 {
    font-size: 32px;
    color: rgb(255, 255, 255);
    -unity-font-definition: resource('Fonts/digital_disco/DigitalDisco');
    white-space: nowrap;
}

.ChoiceGroup SelectableButton {
    background-image: none;
    -unity-background-image-tint-color: rgb(255, 255, 255);
    border-top-width: 5px;
    border-right-width: 5px;
    border-bottom-width: 5px;
    border-left-width: 5px;
    border-left-color: rgba(154, 154, 154, 0.35);
    border-right-color: rgba(154, 154, 154, 0.35);
    border-top-color: rgba(154, 154, 154, 0.35);
    border-bottom-color: rgba(154, 154, 154, 0.35);
    font-size: 32px;
    color: rgb(0, 0, 0);
    flex-grow: 1;
}

.ChoiceGroup SelectableButton.selected {
    background-image: none;
    -unity-background-image-tint-color: rgb(255, 255, 255);
    border-top-width: 5px;
    border-right-width: 5px;
    border-bottom-width: 5px;
    border-left-width: 5px;
    border-left-color: rgb(162, 62, 140);
    border-right-color: rgb(162, 62, 140);
    border-top-color: rgb(162, 62, 140);
    border-bottom-color: rgb(162, 62, 140);
    flex-grow: 1;
    scale: 1.1 1.1;
    transition-property: scale;
    transition-duration: 0.4s;
    transition-timing-function: ease-in;
}

TextBubble.selected {
    transition-timing-function: ease-in-out-elastic;
    transition-duration: 1s;
}

VisualElement.panel.visible {
    flex-grow: 1;
    align-self: stretch;
    background-color: rgba(255, 255, 255, 0);
    -unity-background-scale-mode: stretch-to-fill;
    transition-duration: 0.3s;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    opacity: 1;
    display: flex;
}

SelectableSlider {
    flex-grow: 1;
    align-items: center;
    flex-direction: row;
}

SelectableSlider .unity-text-element {
    -unity-text-align: middle-center;
    -unity-background-image-tint-color: rgba(146, 125, 96, 0.21);
    color: rgb(91, 62, 33);
    background-image: url("project://database/Assets/[SPRITES]/_interactables/[PIXEL]%20general%20store/genstore_enlarged_blank_note.png?fileID=-3837518041837279857&guid=737c92ea2612af84d9c2356c9f05ab09&type=3#enlarged_blank_note_0");
    flex-grow: 0;
    font-size: 24px;
    height: 48%;
    width: 13%;
    flex-shrink: 0;
    -unity-font-definition: resource('Fonts/daniel/daniel');
    border-top-left-radius: -9px;
    border-top-right-radius: -9px;
    border-bottom-right-radius: -9px;
    border-bottom-left-radius: -9px;
    -unity-font-style: bold;
}

