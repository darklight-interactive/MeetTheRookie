}

SelectableButton.selected {
    scale: 1.1 1.1;
    transition-property: scale, -unity-background-image-tint-color, translate, rotate;
    transition-duration: 0.3s, 0.3s, 0.3s, 0.3s;
    border-top-width: 1px;
    border-right-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-left-color: rgb(35, 28, 75);
    border-right-color: rgb(35, 28, 75);
    border-top-color: rgb(35, 28, 75);
    border-bottom-color: rgb(35, 28, 75);
