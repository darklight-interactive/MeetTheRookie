    flex-grow: 0;
    flex-shrink: 0;
    align-self: center;
    align-items: center;
    justify-content: center;
    -unity-background-scale-mode: stretch-to-fill;
    background-image: url("project://database/Assets/[SPRITES]/DialogueUI.png?fileID=-3514209476637312783&guid=cb0772e0a86fdf34f89e41fa59df7756&type=3#DialogueUI_1");
