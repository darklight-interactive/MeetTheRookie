.inky-label {
    font-size: 72px;
    -unity-font-style: normal;
    -unity-text-align: lower-left;
    padding-top: 0;
    padding-right: 25px;
    padding-bottom: 0;
    padding-left: 25px;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    white-space: normal;
    justify-content: center;
    flex-direction: column;
    align-items: center;
    align-self: center;
    flex-wrap: wrap;
    text-overflow: clip;
    overflow: visible;
    -unity-font-definition: url("project://database/Assets/[USER%20INTERFACE]/_fonts/LGGothic.ttf?fileID=12800000&guid=d95d61c132c648c42b6313ed97ed9cf7&type=3#LGGothic");
    flex-grow: 0;
    flex-shrink: 0;
    -unity-background-scale-mode: scale-to-fit;
}

.inky-bubble {
    padding-top: 64px;
    padding-right: 64px;
    padding-bottom: 8px;
    padding-left: 64px;
    position: relative;
