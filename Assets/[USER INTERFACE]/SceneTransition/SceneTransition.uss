.unity-visual-element:enabled {
    transition-duration: 1s;
    background-color: rgb(0, 0, 0);
    width: 100%;
    height: 100%;
    align-self: center;
    justify-content: center;
}

.unity-visual-element {
    background-color: rgba(0, 0, 0, 0);
    transition-duration: 1s;
}

.unity-label {
    color: rgb(255, 255, 255);
    align-items: center;
    justify-content: center;
    align-self: center;
    -unity-text-align: upper-center;
}
